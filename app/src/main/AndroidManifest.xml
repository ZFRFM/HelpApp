<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/launch_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/launch_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimbirSoftPractice"
        tools:targetApi="31">
        <activity
            android:name=".presentation.layouts.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".kotlin.kotlin1.MainActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.layouts.SplashActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.SimbirSoftPractice">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>